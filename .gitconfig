[core]
	editor = vim
[alias]
	br = branch
	st = status -sb
	ll = log --oneline
	ci = commit -am
	co = checkout
	cob = checkout -b
  mm = merge master
	aa = "!git add -u; git add .; git st"
	undo = reset --hard HEAD
	pm = "!git co master && git pull"
	com = "!git co master && git pull && git checkout -b"
[user]
	name = Daniel Cook
	email = dfcook@hotmail.com
[init]
	defaultBranch = main
[push]
	autoSetupRemote = true